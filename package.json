    "zeroclipboard": "^2.3.0"
  },
  "dependencies": {},
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "postinstall": "node ./node_modules/cssstyle/scripts/generate_properties.js",
    "make-docs": "rm -rf ./docs ; ./node_modules/markdown-styles/bin/generate-md --layout github --input README.md --output ./docs ; mv ./docs/README.html ./docs/index.html"
  },
  "jest": {
    "setupFiles": [
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ]
